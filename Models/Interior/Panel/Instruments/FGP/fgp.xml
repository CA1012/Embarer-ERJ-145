<!-- HDG -->

    <animation>
        <type>pick</type>
        <object-name>hdg_set_m1</object-name>
        <action>
            <button>0</button>
			<repeatable>true</repeatable>
			<interval-sec>0.25</interval-sec>
   			<binding>
				 <command>property-adjust</command>
				 <property>autopilot/settings/heading-bug-deg</property>
