<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <fx>
    
    <engine>
      <name>left engine</name>
      <path>Sounds/jet.wav</path>
      <mode>looped</mode>
      <condition>
        <and>
          <property>/engines/engine[0]/running</property>
          <greater-than>
            <property>/sim/current-view/view-number</property>
            <value>0</value>
          </greater-than>
        </and>
      </condition>
      <volume>
        <property>/engines/engine[0]/thrust-lbs</property>
        <factor>0.0004</factor>
        <min>0.0</min>
        <max>0.8</max>
      </volume>
      <position>
        <x>7.3</x>
        <y>-2.1</y>
        <z>-0.09</z>
      </position>
    </engine>

<engine>
<name>left n1</name>
<path>Sounds/whine.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[0]/running</property>
<greater-than>
<property>/sim/current-view/view-number</property>
<value>0</value>
</greater-than>
</and>
</condition>
<volume>
<factor>0.4</factor>
</volume>
<pitch>
<property>/engines/engine[0]/n1</property>
<factor>0.01</factor>
<min>0.0</min>
<max>0.8</max>
</pitch>
<position>
<x>7.3</x>
<y>-2.1</y>
<z>-0.09</z>
</position>
</engine>

<engine>
<name>right engine</name>
<path>Sounds/jet.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[1]/running</property>
<greater-than>
<property>/sim/current-view/view-number</property>
<value>0</value>
</greater-than>
</and>
</condition>
<volume>
<property>/engines/engine[1]/thrust-lbs</property>
<factor>0.0004</factor>
<min>0.0</min>
<max>0.8</max>
</volume>
<position>
<x>7.3</x>
<y>2.1</y>
<z>-0.09</z>
</position>
</engine>

<engine>
<name>right n1</name>
<path>Sounds/whine.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[1]/running</property>
<greater-than>
<property>/sim/current-view/view-number</property>
<value>0</value>
</greater-than>
</and>
</condition>
<volume>
<factor>0.4</factor>
</volume>
<pitch>
<property>/engines/engine[1]/n1</property>
<factor>0.01</factor>
<min>0.0</min>
<max>0.8</max>
</pitch>
<position>
<x>7.3</x>
<y>2.1</y>
<z>-0.09</z>
</position>
</engine>

<engine>
<name>left engine inside</name>
<path>Sounds/jet.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[0]/running</property>
<equals>
<property>/sim/current-view/view-number</property>
<value>0</value>
</equals>
</and>
</condition>
<volume>
<property>/engines/engine[0]/thrust-lbs</property>
<factor>0.0002</factor>
<min>0.0</min>
<max>0.3</max>
</volume>
<position>
<x>7.3</x>
<y>-2.1</y>
<z>-0.09</z>
</position>
</engine>

<engine>
<name>left n1 inside</name>
<path>Sounds/whine.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[0]/running</property>
<equals>
<property>/sim/current-view/view-number</property>
<value>0</value>
</equals>
</and>
</condition>
<volume>
<factor>0.1</factor>
</volume>
<pitch>
<property>/engines/engine[0]/n1</property>
<factor>0.01</factor>
<min>0.0</min>
<max>1.0</max>
</pitch>
<position>
<x>7.3</x>
<y>-2.1</y>
<z>-0.09</z>
</position>
</engine>

<engine>
<name>right engine inside</name>
<path>Sounds/jet.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[1]/running</property>
<equals>
<property>/sim/current-view/view-number</property>
<value>0</value>
</equals>
</and>
</condition>
<volume>
<property>/engines/engine[1]/thrust-lbs</property>
<factor>0.0002</factor>
<min>0.0</min>
<max>0.3</max>
</volume>
<position>
<x>7.3</x>
<y>2.1</y>
<z>-0.09</z>
</position>
</engine>

<engine>
<name>right n1 inside</name>
<path>Sounds/whine.wav</path>
<mode>looped</mode>
<condition>
<and>
<property>/engines/engine[1]/running</property>
<equals>
<property>/sim/current-view/view-number</property>
<value>0</value>
</equals>
</and>
</condition>
<volume>
<factor>0.1</factor>
</volume>
<pitch>
<property>/engines/engine[1]/n1</property>
<factor>0.01</factor>
<min>0.0</min>
<max>1.0</max>
</pitch>
<position>
<x>7.3</x>
<y>2.1</y>
<z>-0.09</z>
</position>
</engine>

<rumble>
<name>rumble</name>
<mode>looped</mode>
<path>Sounds/rumble.wav</path>
<condition>
<or>
<property>/gear/gear[0]/wow</property>
<property>/gear/gear[1]/wow</property>
<property>/gear/gear[2]/wow</property>
</or>
</condition>
      <volume>
        <property>/gear/gear[2]/rollspeed-ms</property>
        <type>log</type>
        <factor>0.12</factor>
      </volume>
</rumble>

<gear1>
<name>gear</name>
<mode>in-transit</mode>
<path>Sounds/gear.wav</path>
<property>/gear/gear[0]/position-norm</property>
<pitch>
<internal>dt_play</internal>
<offset>1.0</offset>
<factor>-0.001</factor>
</pitch>
</gear1>

<gear1>
<name>gear-lock</name>
<path>Sounds/gear-lck.wav</path>
<condition>
<equals>
<property>/gear/gear[0]/position-norm</property>
<value>0</value>
</equals>
</condition>
</gear1>

    <gear1>
      <name>gear-lock</name>
      <path>Sounds/gear-lck.wav</path>
      <condition>
        <equals>
          <property>/gear/gear[0]/position-norm</property>
          <value>1</value>
        </equals>
      </condition>
    </gear1>

<squeal1>
<name>squeal</name>
<path>/Aircraft/Embraer-ERJ145/Sounds/touchdown.wav</path>
<property>/gear/gear[0]/wow</property>
<volume>
<property>/velocities/speed-down-fps</property>
<factor>0.05</factor>
</volume>
<volume>
<property>/velocities/airspeed-kt</property>
<factor>0.01</factor>
</volume>
<volume>
<internal>dt_stop</internal>
<factor>0.05</factor>
<max>1.0</max>
</volume>
<pitch>
<property>/velocities/airspeed-kt</property>
<factor>0.0025</factor>
<offset>1.2</offset>
</pitch>
</squeal1>

<gear2>
<name>gear</name>
<mode>in-transit</mode>
<path>Aircraft/Embraer-ERJ145/Sounds/gear-up.wav</path>
<property>/gear/gear[1]/position-norm</property>
<pitch>
<internal>dt_play</internal>
<offset>1.0</offset>
<factor>-0.001</factor>
</pitch>
</gear2>

<gear2>
<name>gear-lock</name>
<path>Sounds/gear-lck.wav</path>
<condition>
<equals>
<property>/gear/gear[1]/position-norm</property>
<value>0</value>
</equals>
</condition>
</gear2>

    <gear2>
      <name>gear-lock</name>
      <path>Sounds/gear-lck.wav</path>
      <condition>
        <equals>
          <property>/gear/gear[1]/position-norm</property>
          <value>1</value>
        </equals>
      </condition>
    </gear2>

<squeal2>
<name>squeal</name>
<path>/Aircraft/Embraer-ERJ145/Sounds/touchdown.wav</path>
<property>/gear/gear[1]/wow</property>
<volume>
<property>/velocities/speed-down-fps</property>
<factor>0.05</factor>
</volume>
<volume>
<property>/velocities/airspeed-kt</property>
<factor>0.01</factor>
</volume>
<volume>
<internal>dt_stop</internal>
<factor>0.05</factor>
<max>1.0</max>
</volume>
<pitch>
<property>/velocities/airspeed-kt</property>
<factor>0.0025</factor>
<offset>1.2</offset>
</pitch>
</squeal2>

<gear3>
<name>gear</name>
<mode>in-transit</mode>
<path>Sounds/gear.wav</path>
<property>/gear/gear[2]/position-norm</property>
<pitch>
<internal>dt_play</internal>
<offset>1.0</offset>
<factor>-0.001</factor>
</pitch>
</gear3>

<gear3>
<name>gear-lock</name>
<path>Sounds/gear-lck.wav</path>
<condition>
<equals>
<property>/gear/gear[2]/position-norm</property>
<value>0</value>
</equals>
</condition>
</gear3>

    <gear3>
      <name>gear-lock</name>
      <path>Sounds/gear-lck.wav</path>
      <condition>
        <equals>
          <property>/gear/gear[2]/position-norm</property>
          <value>1</value>
        </equals>
      </condition>
    </gear3>

<squeal3>
<name>squeal</name>
<path>/Aircraft/Embraer-ERJ145/Sounds/touchdown.wav</path>
<property>/gear/gear[2]/wow</property>
<volume>
<property>/velocities/speed-down-fps</property>
<factor>0.05</factor>
</volume>
<volume>
<property>/velocities/airspeed-kt</property>
<factor>0.01</factor>
</volume>
<volume>
<internal>dt_stop</internal>
<factor>0.05</factor>
<max>1.0</max>
</volume>
<pitch>
<property>/velocities/airspeed-kt</property>
<factor>0.0025</factor>
<offset>1.2</offset>
</pitch>
</squeal3>

    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/left-flap-pos-norm</property>
      <volume>
        <factor>0.15</factor>
      </volume>
    </flap>
    
    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/right-flap-pos-norm</property>
      <volume>
        <factor>0.25</factor>
      </volume>
    </flap>
    
    <spoiler>
      <name>spoilers</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/left-lower-spoiler-pos-norm</property>
      <volume>
        <factor>0.15</factor>
      </volume>
    </spoiler>
    
    <spoiler>
      <name>spoilers</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/left-upper-spoiler-pos-norm</property>
      <volume>
        <factor>0.15</factor>
      </volume>
    </spoiler>
    
    <spoiler>
      <name>spoilers</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/right-lower-spoiler-pos-norm</property>
      <volume>
        <factor>0.25</factor>
      </volume>
    </spoiler>
    
    <spoiler>
      <name>spoilers</name>
      <mode>in-transit</mode>
      <path>Sounds/flaps.wav</path>
      <property>/surface-positions/right-upper-spoiler-pos-norm</property>
      <volume>
        <factor>0.25</factor>
      </volume>
    </spoiler>

<wind>
<name>wind</name>
<mode>looped</mode>
<path>Sounds/wind.wav</path>
<property>/velocities/airspeed-kt</property>
<volume>
<property>/position/altitude-ft</property>
<factor>-0.000015</factor>
<offset>1.0</offset>
<min>0.1</min>
<max>1.0</max>
</volume>
<volume>
<property>/velocities/airspeed-kt</property>
<factor>0.0015</factor>
<min>0.03</min>
<max>0.25</max>
</volume>
<pitch>
<property>/velocities/airspeed-kt</property>
<factor>0.01</factor>
<offset>1.25</offset>
</pitch>
</wind>

    <crash>
      <name>intense ground contact</name>
      <mode>transient</mode>
      <path>Sounds/crash.wav</path>
      <property>sim/crashed</property>
      <volume>
        <factor>1</factor>
      </volume>
      <pitch>
        <factor>1.5</factor>
      </pitch>
    </crash>

    <switch>
      <name>click on</name>
      <mode>once</mode>
      <path>Aircraft/Embraer-ERJ145/Sounds/click.wav</path>
      <condition>
        <property>/sim/sound/switch</property>
      </condition>
      <volume>
        <factor>0.45</factor>
      </volume>
    </switch>

    <switch>
      <name>click off</name>
      <mode>once</mode>
      <path>Aircraft/Embraer-ERJ145/Sounds/click.wav</path>
      <condition>
        <not>
          <property>/sim/sound/switch</property>
        </not>
      </condition>
      <volume>
        <factor>0.45</factor>
      </volume>
    </switch>

    <speedbrake>
      <name>speedbrake</name>
      <mode>in-transit</mode>
      <path>Sounds/whine.wav</path>
      <condition>
        <less-than>
          <property>/surface-positions/speedbrake-pos-norm</property>
          <value>0.8</value>
        </less-than>
        <greater-than>
          <property>/surface-positions/speedbrake-pos-norm</property>
          <value>0.18</value>
        </greater-than>
      </condition>
      <volume>
        <factor>0.45</factor>
      </volume>
    </speedbrake>

    <door>
      <name>Crew door</name>
      <path>Sounds/door_close.wav</path>
      <condition>
        <equals>
          <property>/instrumentation/doors/crew/position-norm</property>
          <value>0.0</value>
        </equals>
      </condition>
      <volume>
        <factor>0.4</factor>
      </volume>
    </door>

</fx>

</PropertyList>
